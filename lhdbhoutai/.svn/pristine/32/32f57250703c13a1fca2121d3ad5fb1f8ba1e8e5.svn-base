	color: #f6eeee;
}

.tableall-x{
	border: 1px solid #b4b4b4;
}

.topindextite {
	width: 100%;
	background: #e0e0e0;
	height: 72px;
	line-height: 72px;
	border-radius: 9px;
	margin-bottom: 10px;
	border: 1px solid #b4b4b4;
}

.navTops {
	width: 300px;
	margin: 0px auto;
}

.navTops ul {
	list-style: none;
}

.navTops ul li {
	float: left;
	display: inline;
	vertical-align: middle;
	width: 64px;
	margin-right: 60px;
	cursor: pointer;
}

.topnavicon {
	width: 24px;
	height: 24px;
	background: url(../image/userpay.png) top center no-repeat;
	vertical-align: middle;
	float: left;
	margin-bottom: 3px;
}

.topnavicon2 {
	width: 24px;
	height: 24px;
	background: url(../image/userinfos.png) top center no-repeat;
	vertical-align: middle;
	float: left;
	margin-bottom: 3px;
}

.navTops ul li span {
	line-height: 16px;
	font-size: 16px;
	color: #333333;
	display: block;
	zoom: 1;
	width: 100%;
	text-align: center;
}

.toptitlist .bot {
	margin-top: 3px;
	height: 5px;
}

.currentItem .bot {
	background: #01A7AD;
}

.divcontentshow {
	margin: 0px 50px;
	height: 159px;
	border: 1px solid gray;
	margin-bottom: 1em;
}

.model_p {
	width: 100%;
	float: left;
	text-align: center;
	font-size: 2em;
	margin: 2em 0em;
}

.divcontentshow span {
	width: 100%;
	float: left;
	text-align: left;
	font-size: 1.1em;
	margin-bottom: 0.5em;
	padding-left: 0.5em;
}

.chattime{
	float: right;
}

.iphoneicon{
	background:url(../image/phone.gif) no-repeat 60% 50%;
}
 .thistdvalue{
  height:18px;
  background:url(../image/copy.png) no-repeat;
 border:none;
  }

/*textarea  css*/
.usertextarea{
	width:255px;
	height:180px;
    resize: none;
}
.rotate90{
	transform:rotate(-90deg);
	-ms-transform:rotate(-90deg); 	/* IE 9 */
	-moz-transform:rotate(-90deg); 	/* Firefox */
	-webkit-transform:rotate(-90deg); /* Safari 和 Chrome */
	-o-transform:rotate(-90deg); 	/* Opera */
	margin-top: -20px;
	min-width: 200px;
}
#preview{width:90px;height:90px;border:1px solid #000;overflow:hidden;float: left;}
#imghead {filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod=image);}
.table th, .table td {
	min-width: 75px;
}
